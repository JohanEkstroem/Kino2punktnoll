<div class="container">
    <h1>Kommande visningar</h1>
    <div class="row">
        {{#each movies }}
            <div class="card m-2 p-0">
                <p class="card-header">{{ this.attributes.title }}</p>
                <a href="/movies/{{ this.id }}"><img class="card-img-bottom" src="{{{ this.attributes.image.url }}}"></a>
            </div>
        {{/each}}
    </div>
</div>